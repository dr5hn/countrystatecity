{
  "name": "@countrystatecity/countries-browser",
  "version": "1.0.0",
  "description": "Browser-native countries, states, and cities database with lazy loading via fetch API",
  "keywords": [
    "country-state-city",
    "countries states cities",
    "countries states cities database",
    "country state city",
    "country state city database",
    "geolocation",
    "country",
    "state",
    "city",
    "location",
    "geography",
    "iso",
    "typescript",
    "browser",
    "frontend",
    "vite",
    "webpack",
    "react",
    "vue",
    "svelte",
    "fetch",
    "lazy-loading",
    "cdn"
  ],
  "author": "dr5hn",
  "license": "ODbL-1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dr5hn/countrystatecity.git",
    "directory": "packages/countries-browser"
  },
  "homepage": "https://github.com/dr5hn/countrystatecity/tree/main/packages/countries-browser#readme",
  "bugs": {
    "url": "https://github.com/dr5hn/countrystatecity/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./data/*": "./dist/data/*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm generate-data && tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:browser": "playwright test",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "generate-data": "node scripts/generate-browser-data.cjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.19.19",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

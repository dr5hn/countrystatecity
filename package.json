{
  "name": "@countrystatecity/monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for @countrystatecity/* package ecosystem",
  "scripts": {
    "build": "pnpm --filter \"@countrystatecity/*\" run build",
    "test": "pnpm --filter \"@countrystatecity/*\" run test",
    "dev": "pnpm --filter \"@countrystatecity/*\" run dev",
    "lint": "pnpm --filter \"@countrystatecity/*\" run lint",
    "clean": "pnpm --filter \"@countrystatecity/*\" exec rm -rf dist node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
